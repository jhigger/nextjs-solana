generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Status {
  id         Int          @id @default(autoincrement())
  name       String
  submission Submission[]
}

model Submission {
  address  String @id
  email    String
  discord  String
  project  String
  statusId Int    @default(1)
  status   Status @relation(fields: [statusId], references: [id])

  @@index([address])
}
